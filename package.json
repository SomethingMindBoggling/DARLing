{
  "private": true,
  "name": "dar-tool-api",
  "version": "0.0.0",
  "description": "API for the Data Availabity Ranker Tool",
  "repository": "jacobwindsor/dar-tool-api",
  "author": "Jacob Windsor",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "jsnext:main": "index.es6.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base"
  },
  "dependencies": {
    "agenda": "^0.9.1",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.17.2",
    "chai-as-promised": "^6.0.0",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "jwks-rsa": "^1.1.1",
    "mongorito": "^3.0.1",
    "node-fetch": "^1.7.0",
    "oauth2orize": "^1.8.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "rxjs": "^5.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "del": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "rollup": "^0.26.2",
    "rollup-plugin-babel": "^2.4.0",
    "sinon": "^2.0.0-pre"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --recursive --timeout 150000 --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo jacobwindsor/dar-tool-api",
    "start:docs": "easystatic start docs",
    "start": "babel-node src/server.js --presets es2017"
  }
}
